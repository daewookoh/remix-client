    "remix-auth-github": "^1.7.0",
    "isbot": "^5.1.0",
    "cloudinary": "^2.5.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.17.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/bcryptjs": "^2.4.6",
